    "canvas": "^2.11.2",
    "child_process": "^1.0.2",
    "execa": "^6.0.0",
    "express": "^4.17.2",
    "face-api.js": "^0.22.2",
    "find-remove": "^4.0.4",
    "fvi-node-face-api-rtsp-stream": "^0.0.3",
    "http": "0.0.1-security",
    "moment": "^2.29.1",
    "node-rtsp-recorder": "^1.0.3",
    "node-rtsp-stream": "^0.0.9",
    "nodemon": "^2.0.15",
    "rtsp-downloader": "^1.0.2",
    "rtsp-video-recorder": "^2.0.2-beta.1",
    "rtsp-video-recorder-ffmpeg-options": "^1.4.0-alpha.3",
    "samba-client": "^4.0.0",
    "websocket": "^1.0.34"
