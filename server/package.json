    "do-install": "build-opencv build",
    "serve": "nodemon server.js",
    "start": "nodemon server.js"
  },
  "dependencies": {
    "@network-utils/arp-lookup": "^2.0.1",
    "@u4/opencv4nodejs": "^6.5.2",
